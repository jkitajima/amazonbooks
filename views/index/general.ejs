<!-- Freq produto / categoria -->
<script>
    var aux = JSON.parse(`<%- freqProCat %>`)
    var series = [{data: []}]
    var categories = []
    aux.forEach((r)=>{
        series[0].data.push(r.data)
        categories.push(r.name);
    })

    var options = {
          series: series,
          chart: {
          type: 'bar',
          height: 350
        },
        animations: {
            enabled: true,
            speed: 175
        },
    
        plotOptions: {
          bar: {
            borderRadius: 4,
            horizontal: true,
          }
        },
        dataLabels: {
          enabled: false
        },
        xaxis: {
          categories: categories,
        }
    };

    var chartProCat = new ApexCharts(document.querySelector("#chartProCat"), options);
    chartProCat.render();
</script>

