<!-- Review x Pages -->
<script>
    var seriesRevPag = `<%- seriesRevPag %>`

    var optionsRevPag = {
        series: JSON.parse(seriesRevPag),
        chart: {
            height: 350,
            type: 'scatter',
            zoom: {
                enabled: true,
                type: 'xy'
            },
            animations: {
                enabled: false,
                speed: 175
            }
        },

        xaxis: {
            tickAmount: 10,
            labels: {
                formatter: function(val) {
                    return parseFloat(val).toFixed(1)
                }
            }
        },
        yaxis: {
            tickAmount: 7
        },
        theme: {
            palette: 'palette7'
        }
    };

    var chartRevPag = new ApexCharts(document.querySelector("#chartRevPag"), optionsRevPag);
    chartRevPag.render();
</script>

